// Runtime 63 ms Beats 85.5% || Memory 42.2 MB Beats 32.62%

var repeatedCharacter = function(s) {
    
    let lowest = s.length

    for( let i = 0; i < s.length; i++ ){
        for( let j = i + 1; j < s.length; j++ ){
            if( s[j] === s[i] && j < lowest ){
                lowest = j
            }
        }
    }

    return s[lowest]
};

// Runtime 66 ms Beats 78.83% || Memory 42.1 MB Beats 32.62%

var repeatedCharacter = function(s){

    let charObj = {}

    for( let char of s ){
        if( charObj[char] ){
            return char
        } else{
            charObj[char] = true
        }
    }

}
