//Runtime 90 ms Beats 79.31% || Memory 45.4 MB Beats 9.85%

var insertionSortList = function(head) {
    
    let vals = []
    while( head ){
        vals.push(head.val)
        head = head.next
    }

    vals = insertionSort(vals)
    
    let node = tail = new ListNode()
    for( let val of vals){
        node.next = new ListNode(val)
        node = node.next
    }

    return tail.next

};

var insertionSort = function(arr){
    let n = arr.length
    
    for( let i = 1; i < n; i++ ){
        let key = arr[i]
        let j = i - 1
        while( (j >= 0) && (arr[j] > key) ){
            arr[j + 1] = arr[j]
            j--
        }
        arr[j + 1] = key
    }
    
    return arr
}
