// Runtime 62 ms Beats 48.83% || Memory 42 MB Beats 68.27%

var heightChecker = function(heights) {
    let expected = [...heights].sort((a,b) => a - b)
    let result = 0
    for( let i = 0; i < expected.length; i++ ){
        if( expected[i] != heights[i] ){
            result++
        }
    }
    return result
};
