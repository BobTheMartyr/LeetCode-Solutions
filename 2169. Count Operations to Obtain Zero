// Runtime 59 ms Beats 91.39% || Memory 42.2 MB Beats 44.37%


var countOperations = function(num1, num2) {
    let count = 0
    while( num1 > 0 && num2 > 0 ){
        if( num1 >= num2 ){
            num1 -= num2
            count++
        } else {
            num2 -= num1
            count++
        }
    }
    return count
};
