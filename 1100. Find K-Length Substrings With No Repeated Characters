var numKLenSubstrNoRepeats = function(s, k) {
    if( k > s.length ) return 0
    let word = ''
    let count = 0
    for( let i = 0; i < s.length; i++ ){
        for( let j = i; j < s.length; j++ ){
            if( word.includes(s[j]) && word.length != k ){
                word = ''
                break
            }
            if( word.length == k ){
                let temp = new Set(word)
                if( temp.size == k ){
                    count += 1
                    console.log(word)
                    word = ''
                }
                break
            } else {
                word += s[j]
            }
        }
    }
    return count

};
